<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<mapper namespace="com.friday.hgj.dao.ComplainDao">

    <insert id="insert" parameterType="Complain">
		insert into complain(orderId,complainantUserId,defendantUserId,operateTime,operateId) 
		values(#{orderId},#{complainantUserId},#{defendantUserId},#{operateTime},#{operateId})
	</insert>
	
</mapper>
