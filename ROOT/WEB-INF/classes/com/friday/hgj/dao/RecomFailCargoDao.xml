<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<mapper namespace="com.friday.hgj.dao.RecomFailCargoDao">
     <insert id="insert" parameterType="com.friday.hgj.model.RecomFailCargo" useGeneratedKeys="true" keyProperty="id">
		insert into recomfailcargo(userId,failResion,todealTime,todealId,cargoId,status,type) 
		values(#{userId},#{failResion},#{todealTime},#{todealId},#{cargoId},#{status},#{type})
	</insert>

	
</mapper>
